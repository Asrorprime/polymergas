      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/react": "^16.9.35",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.2",
    "apexcharts": "3.19.2",
    "availity-reactstrap-validation": "^2.6.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "chroma-js": "^2.1.0",
    "dva-model-extend": "^0.1.2",
    "firebase": "^7.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-apexcharts": "1.3.7",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0",
    "react-js-pagination": "^3.0.3",
    "react-paginate": "6.3.2",
    "react-pose": "4.0.10",
    "react-select": "3.1.0",
    "react-select-search": "^2.0.3",
    "react-sparklines": "1.7.0",
    "react-toastify": "^6.0.5",
    "reactstrap": "^8.4.1",
    "umi": "^3.2.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/react-js-pagination": "^3.0.3",
    "@types/reactstrap": "^8.4.2"
  },
  "presets": [
    "@babel/preset-flow",
    "@babel/env",
    "@babel/react"
  ]
